# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET		/					controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET		/assets/*file		controllers.Assets.at(path="/public", file)

GET		/koch				controllers.Application.koch

GET		/foo				controllers.Application.foo

POST	/ajax				controllers.Application.slCalls

GET		/checkOption		controllers.Application.checkOption

#GET		/woot				controllers.Application.woot

#GET	/kochFile		    controllers.Application.kochFile

#GET	/expr			    controllers.Application.expr

#GET	/parseExpr		    controllers.Application.expr2Json(in : String)